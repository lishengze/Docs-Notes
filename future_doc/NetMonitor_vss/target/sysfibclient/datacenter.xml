	<!-- 会员/席位登陆告警 -->
	<LoginWarningEvents objectid="TRADE.PuDian.app.tkernel.1">
		<!--
		 定义：在interval秒之内若某会员/席位的登陆次数超过threshold则发送eventname类型的告警
		 登陆次数会在交易日切换时重置，因此将interval设置为大于24小时，即可表示整个交易日的登陆次数
		 warningap制定了两次告警之间的最小时间间隔
		 -->
		 <!-- user/participant表示是否针对席位/会员启用告警功能，默认值是启用 -->
		<event eventname="TotalLoginWarning" user="1" participant="0" interval="86400" threshold="10" warninggap="60"/>
		<event eventname="IntervalLoginWarning" user="1" participant="1" interval="120" threshold="5" warninggap="60"/>
	</LoginWarningEvents>
