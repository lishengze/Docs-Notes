<OFP author="王肇东" head="OFP" version="1" comment="上海期货交易所原有通讯协议" isUp="no" isDown="no" isPrivate="no" isPublic="no" multifield="no" v1use="yes" v2use="yes">
	<packages>
		<package name="LoginRequest" vary="0x0401" comm="login" comment="用户登录指令" isUp="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
			<field name="Transferid" type="Number" length="8" comment="私有流最大编号"/>
		</package>
		<package name="LoginResponse" vary="0x0501" comm="OK" comment="用户登录应答" isDown="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="Password" type="String" length="40" comment="口令"/>
			<field name="Usernm" type="String" length="8" comment="交易员姓名"/>
			<field name="Localno0" type="String" length="8" comment="交易员用最大本地编号"/>
			<field name="Time" type="String" length="8" comment="交易系统时间"/>
			<field name="Sysstat" type="Number" length="8" comment="交易系统状态"/>
			<field name="Membid" type="String" length="8" comment="所属会员"/>
			<field name="Section" type="String" length="2" comment="当前交易代码"/>
			<field name="Sstatus" type="Char" comment="交易节状态"/>
			<field name="Localno0" type="String" length="8" comment="交易员用最大报单本地编号"/>
			<field name="Transferid" type="Number" length="8" comment="私有流最大编号"/>
		</package>
		<package name="LoginErrorResponse" vary="0x0501" comm="ERROR" comment="用户登录失败应答" isDown="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
			<field name="Transferid" type="Number" length="8" comment="私有流最大编号"/>
		</package>
		<package name="LogoutRequest" vary="0x0407" comm="logout" comment="用户登录退出指令" isUp="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
		</package>
		<package name="LogoutResponse" vary="0x0507" comm="OK" comment="用户登录退出应答" isDown="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
		</package>
		<package name="LogoutErrorResponse" vary="0x0507" comm="ERROR" comment="用户登录退出失败应答" isDown="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
		</package>
		<package name="PasswordUpdateRequest" vary="0x0220" comm="modpwd" comment="用户口令修改指令" isUp="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="OldPasswd" type="String" length="40" comment="旧口令"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
		</package>
		<package name="PasswordUpdateResponse" vary="0x0320" comm="OK" comment="用户口令修改应答" isDown="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="OldPasswd" type="String" length="40" comment="旧口令"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
		</package>
		<package name="PasswordUpdateErrorResponse" vary="0x0320" comm="ERROR" comment="用户口令修改失败应答" isDown="yes">
			<field name="Userid" type="String" length="15" comment="交易员"/>
			<field name="OldPasswd" type="String" length="40" comment="旧口令"/>
			<field name="Passwd" type="String" length="40" comment="口令"/>
		</package>
		<package name="ForceExit" vary="0x0513" comm="killlog" comment="强制退出" isDown="yes"/>
