			<field name="Content" type="String" length="70" comment="警告信息"/>
		</package>
		<package name="SectionStart" vary="0x0522" comm="" comment="系统节开盘" isDown="yes"/>
		<package name="SectionStop" vary="0x0523" comm="" comment="系统节收盘" isDown="yes"/>
		<package name="OpenAuctionStart" vary="0x0524" comm="" comment="系统节开盘集合竞价开始" isDown="yes"/>
		<package name="OpenAuctionSectionStop" vary="0x0525" comm="" comment="系统节开盘集合竞价结束" isDown="yes"/>
