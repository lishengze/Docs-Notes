			<field name="Sbalance" type="Number" length="8" comment="卖保值额度剩余吨数"/>
		</package>
		<package name="QryHedgeFailResponse" vary="0x0824" comm="ERROR" comment="保值额度查询失败应答" isDown="yes">
			<field name="Membid" type="String" length="8" comment="查询起始会员"/>
			<field name="Membidt" type="String" length="8" comment="查询结束会员"/>
			<field name="Commid" type="String" length="8" comment="查询起始交易品种"/>
			<field name="Commidt" type="String" length="8" comment="查询结束交易品种"/>
		</package>
		<package name="QryClientRequest" vary="0x1010" comm="eqmem" comment="客户资料查询指令" isUp="yes">
			<field name="Membid" type="String" length="8" comment="查询起始会员"/>
			<field name="Membidt" type="String" length="8" comment="查询结束会员"/>
		</package>
		<package name="QryClientResponse" vary="0x1110" comm="OK" comment="客户资料查询应答" isDown="yes" multifield="yes">
			<field name="Rstus" type="Char" comment="未用"/>
			<field name="Custid" type="String" length="8" comment="客户编码"/>
			<field name="Custnm" type="String" length="50" comment="客户名称"/>
			<field name="Custad" type="String" length="50" comment="客户地址"/>
