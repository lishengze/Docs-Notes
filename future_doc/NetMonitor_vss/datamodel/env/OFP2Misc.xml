		<package name="MBackOrder" vary="0x0701" comm="OK" comment="报单修改提醒" isPrivate="yes">
			<field name="mqid" type="Number" length="8" comment="私有流序列号"/>
			<field name="cmd" type="Number" length="4" comment="信息类型"/>
			<field name="Orderid" type="String" length="8" comment="报单系统编号"/>
			<field name="Commid" type="String" length="6" comment="交易品种"/>
			<field name="Orderp" type="Number" length="6" comment="申报价格"/>
			<field name="Orderv" type="Number" length="6" comment="申报手数"/>
			<field name="Custid" type="String" length="8" comment="客户编码"/>
			<field name="Userid" type="String" length="15" comment="交易员编码"/>
			<field name="Lorderid" type="String" length="8" comment="本地编号"/>
			<field name="Posit" type="Char" comment="买卖方向"/>
			<field name="Closef" type="Char" comment="开平仓标志"/>
			<field name="Tjbz" type="Char" comment="投机保值标志"/>
