			<field name="Membidt" type="String" length="8" comment="查询结束会员"/>
		</package>
		<package name="QryDepositResponse" vary="0x0307" comm="OK" comment="会员保证金查询应答" isDown="yes">
			<field name="Membid" type="String" length="8" comment="会员"/>
			<field name="Margit" type="Float" comment="日初资金"/>
			<field name="Credii" type="String" length="15" comment="未使用"/>
			<field name="Buymar" type="Float" comment="买入使用资金"/>
			<field name="Sellma" type="Float" comment="卖出使用资金"/>
			<field name="Totall" type="Float" comment="开仓使用资金"/>
			<field name="Closec" type="Float" comment="剩余可用资金"/>
		</package>
		<package name="QryDepositErrorResponse" vary="0x0307" comm="ERROR" comment="会员保证金查询失败应答" isDown="yes">
			<field name="Membid" type="String" length="8" comment="查询起始会员"/>
			<field name="Membidt" type="String" length="8" comment="查询结束会员"/>
		</package>
		<package name="QryHedgeRequest" vary="0x0724" comm="eqbzsq" comment="保值额度查询指令" isUp="yes">
			<field name="Membid" type="String" length="8" comment="查询起始会员"/>
			<field name="Membidt" type="String" length="8" comment="查询结束会员"/>
			<field name="Commid" type="String" length="8" comment="查询起始交易品种"/>
			<field name="Commidt" type="String" length="8" comment="查询结束交易品种"/>
		</package>
