			<field name="Compd" type="String" length="8" comment="撮合日期"/>
			<field name="Compt" type="String" length="8" comment="撮合时间"/>
			<field name="Filler" type="String" length="20" comment="未用"/>
		</package>
		<package name="QryDepositRequest" vary="0x0207" comm="eqmag" comment="会员保证金查询指令" isUp="yes">
