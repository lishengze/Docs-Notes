			<field name="Commid" type="String" length="6" comment="交易品种代码"/>
			<field name="Commnm" type="String" length="8" comment="交易品种名称"/>
			<field name="Delim" type="String" length="6" comment="交割月份，格式为yymm+space(2)"/>
			<field name="Abbr" type="String" length="2" comment="大类品种的数字编码，如'01'为铜"/>
			<field name="Trade" type="String" length="6" comment="每手数量（吨）"/>
			<field name="Tick" type="String" length="6" comment="最小变动价位"/>
			<field name="Daill" type="String" length="6" comment="未用，应该是涨跌停板率"/>
			<field name="Closed" type="String" length="8" comment="未用，应该是保值平仓允许开始日期"/>
