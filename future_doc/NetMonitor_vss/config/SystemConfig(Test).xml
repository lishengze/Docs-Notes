		<event name="NotActive" FullName="进程不活跃" type="app" level="error" GiveupTime="60" Control="nowbetween(&quot;08:50:00&quot;,&quot;16:00:00&quot;) &amp;&amp; (getmaincentername()==&quot;PuDian&quot;) " Expr="(^@PuDian.app.compositor.1:IsActive &lt; (now-getperiod(&quot;IsActive&quot;)*3) )" Comment="有1分钟没有收到新的IsActive信息" ObjectId="PuDian.app.compositor.1" ObjectAttr="IsActive"/>
		<Event name="UserSessionFastLost" FullName="用户会话快速丢失" type="app" level="error" GiveupTime="60" Control="nowbetween(&quot;09:00:00&quot;,&quot;10:15:00&quot;) | nowbetween(&quot;10:30:00&quot;,&quot;11:30:00&quot;) | nowbetween(&quot;13:30:00&quot;,&quot;14:10:00&quot;) | nowbetween(&quot;14:10:00&quot;,&quot;15:00:00&quot;)" Expr="(@UserSessionSize-#UserSessionSize) &lt; -20" Comment="在最近两次会话采样中，丢失了20个会话"/>
		<Event name="OrderNotIncrease" FullName="报单无增长" type="app" level="fatal" GiveupTime="60" Control="nowbetween(&quot;09:00:00&quot;,&quot;10:15:00&quot;) | nowbetween(&quot;10:30:00&quot;,&quot;11:30:00&quot;) | nowbetween(&quot;13:30:00&quot;,&quot;14:10:00&quot;) | nowbetween(&quot;14:10:00&quot;,&quot;15:00:00&quot;)" Expr="@OrderSize &lt;= $OrderSize(now-3)" Comment="本次采样的ordersize小于等于3秒前的"/>
		<Event name="DiskNotifing" FullName="硬盘使用率提示" type="app" level="notify" GiveupTime="900" Control="" Expr="@DiskUsage&gt;=20 &amp;&amp; @DiskUsage&lt;50 " Comment="硬盘使用率超过20%"/>
		<Event name="DiskWarning" FullName="硬盘使用率比较高" type="app" level="warning" GiveupTime="900" Control="" Expr="@DiskUsage&gt;=50 &amp;&amp; @DiskUsage&lt;80 " Comment="硬盘使用率超过50%"/>
		<Event name="InsufficientDisk" FullName="硬盘不足" type="app" level="error" GiveupTime="900" Control="" Expr="@DiskUsage&gt;=80 &amp;&amp; @DiskUsage&lt;95 " Comment="硬盘使用率超过80%"/>
		<Event name="DiskDisaster" FullName="硬盘不足灾难" type="app" level="fatal" GiveupTime="900" Control="" Expr="@DiskUsage&gt;=95" Comment="硬盘使用率超过95%"/>
		<Event name="CPUBusy" FullName="CPU比较忙" type="app" level="warning" GiveupTime="60" Control="" Expr="@CPUUsage&gt;=20 &amp;&amp; @CPUUsage&lt;50" Comment="CPU使用率超过20%"/>
		<Event name="CPUTired" FullName="CPU太忙" type="app" level="error" GiveupTime="60" Control="" Expr="@CPUUsage&gt;=50" Comment="CPU使用率超过50%"/>
		<Event name="MemoryWaring" FullName="内存使用率比较高" type="app" level="warning" GiveupTime="60" Control="" Expr="@MemoryUsage&gt;=50 &amp;&amp; @MemoryUsage&lt;80" Comment="内存使用率超过50%"/>
		<Event name="InsufficientMemory" FullName="内存不足" type="app" level="error" GiveupTime="60" Control="" Expr="@MemoryUsage&gt;=80 &amp;&amp; @MemoryUsage&lt;95" Comment="内存使用率超过80%"/>
		<Event name="MemoryDisaster" FullName="内存不足灾难" type="app" level="fatal" GiveupTime="60" Control="" Expr="@MemoryUsage&gt;=95" Comment="内存使用率超过95%"/>
		<Event name="MemoryDatabaseWarning" FullName="内存数据库警告" type="app" level="warning" GiveupTime="86400" Control="nowbetween(&quot;08:30:00&quot;,&quot;15:00:00&quot;)" Expr="(@MemoryDatabaseUsage &gt; 50) | (@MDBBlockUsage &gt; 50)" Comment="内存数据库空间或者块数超过50%"/>
